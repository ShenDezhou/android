<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rock.androidnetdemo"
    android:versionCode="1"
    android:versionName="1.0" >
<!-- 使用网络需要添加InterNet权限
		在Android里面使用硬件的操作，或是关于权限类的操作，都需要添加权限 -->
    <uses-permission android:name="android.permission.INTERNET"/>
    
    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:name="com.rock.androidnetdemo.DemoApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            
        </activity>
        <activity android:name="com.rock.androidnetdemo.SecondActivity"/>
        
        <!--  -->
        <activity android:name="com.rock.androidnetdemo.OnePage">
            
            
            
        </activity>
        
        <activity android:name="com.rock.androidnetdemo.WebViewActivity"></activity>
        
        <activity android:name="com.rock.androidnetdemo.ListViewActivity">
            
        </activity>
        
        <activity android:name="com.rock.androidnetdemo.VolleyActivity">
            
            
        </activity>
        
        <activity android:name="com.rock.androidnetdemo.FragActivity">
            
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            
        </activity>
        
    </application>

</manifest>
